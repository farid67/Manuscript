@article{Bai2017a,
abstract = {The purpose of this study was to conduct a comprehensive study of the clinical correlation between the alpha-fetoprotein (AFP) level at diagnosis and pathological grades, progression, and survival of patients with hepatocellular carcinoma (HCC). A total of 78,743 patients in Surveillance, Epidemiology, and End Results Program (SEER)-registered HCC was analyzed. The AFP test results for patients with HCC were mainly recorded as AFP-negative and AFP-positive. Logistic regression analysis revealed that the AFP level at diagnosis was an independent risk factor of pathological grade (odds ratio [OR], 2.559; 95{\%} confidence interval [CI], 2.075-3.157; P {\textless} 0.001), TNM-7 stage (OR, 2.794; CI, 2.407-3.242; P {\textless} 0.001), and tumor size (OR, 1.748; 95{\%} CI, 1.574-1.941; P {\textless} 0.001). Multivariable Cox regression analyses identified AFP level as an independent predictor of survival risk of patients with HCC who did not undergo surgery (hazard ratio [HR], 1.660; 95{\%} CI, 1.534-1.797; P {\textless} 0.001), and those who underwent surgery (HR, 1.534; 95{\%} CI, 1.348-1.745; P {\textless} 0.001). The AFP level at diagnosis was an independent risk predictor associated with pathological grade, progression, and survival. Further, surgery may not significantly reverse the adverse effects of AFP-positive compared with AFP-negative.},
author = {Bai, Dou Sheng and Zhang, Chi and Chen, Ping and Jin, Sheng Jie and Jiang, Guo Qing},
doi = {10.1038/s41598-017-12834-1},
file = {:home/fouhmich/Documents/Biblio/medical/41598{\_}2017{\_}Article{\_}12834.pdf:pdf},
issn = {20452322},
journal = {Scientific Reports},
number = {1},
pages = {1--9},
pmid = {28993684},
publisher = {Springer US},
title = {{The prognostic correlation of AFP level at diagnosis with pathological grade, progression, and survival of patients with hepatocellular carcinoma}},
url = {http://dx.doi.org/10.1038/s41598-017-12834-1},
volume = {7},
year = {2017}
