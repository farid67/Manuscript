	journal = {IEEE Transactions on Biomedical Engineering},
	keywords = {Generalized Hough transform (GHT),Liver segmentation,Minimum s-t cut,Principal component analysis (PCA),Statistical shape model (SSM)},
	number = {10 PART 2},
