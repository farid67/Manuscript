file = {:home/fouhmich/Downloads/Bray{\_}et{\_}al-2018-CA{\%}3A{\_}A{\_}Cancer{\_}Journal{\_}for{\_}Clinicians.pdf:pdf},
isbn = {0000000000},
issn = {00079235},
journal = {CA: A Cancer Journal for Clinicians},
keywords = {Epidemiology,Lesion,NCI,SEER,Sarcoma},
