doi = {10.1016/j.mri.2012.06.010},
eprint = {NIHMS150003},
file = {:home/fouhmich/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Kumar et al. - 2012 - Radiomics The process and the challenges.pdf:pdf},
