	file = {:home/farid/Documents/These/Biblio/Segmentation{\_}of{\_}the{\_}Liver{\_}Using{\_}the{\_}Deformable{\_}Con.pdf:pdf},
	isbn = {978-3-540-30027-4},
	number = {January},
	pages = {570--581},
	publisher = {Springer Berlin Heidelberg},
	series = {Lecture Notes in Computer Science},
	title = {{Segmentation of the Liver Using the Deformable Contour Method on CT Images}},
	url = {http://www.springerlink.com/content/f730jx3363276h43 http://link.springer.com/10.1007/11581772 http://link.springer.com/10.1007/11581772{\_}50},
	volume = {3767},
