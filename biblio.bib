eprint = {NIHMS150003},
file = {:home/fouhmich/Downloads/rusk2009.pdf:pdf},
isbn = {1361-8423 (Electronic)$\backslash$r1361-8415 (Linking)},
issn = {13618415},
journal = {Medical Image Analysis},
keywords = {Automatic segmentation,Contrast-enhanced CT,Liver contouring,Multi-phase images,Region-growing},
number = {6},
pages = {871--882},
