journal = {Scientific Reports},
number = {December},
pages = {1--10},
pmid = {26639025},
publisher = {Nature Publishing Group},
title = {{Deciphering genomic underpinnings of quantitative MRI-based radiomic phenotypes of invasive breast carcinoma}},
url = {http://dx.doi.org/10.1038/srep17787},
volume = {5},
year = {2015}
}

